{
  "name": "initialize_webproject",
  "version": "0.0.0",
  "authors": [
    "Hugo Est√©vez Rocha"
  ],
  "description": "Bower file for creating the basic structure of a web project.",
  "moduleType": [
    "node"
  ],
  "keywords": [
    "bower",
    "gulp",
    "npm"
  ],
  "license": "MIT",
  "homepage": "www.hugoer.com",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
      "jquery" : "~2.1.4",
      "normalize-css" : "~3.0.3",
      "bootstrap-sass" :"~3.3.4"
  },
  "install": {
        "path": {
            "js": "src/js/vendor",
            "scss": "src/scss/vendor/",
            "css": "src/css/vendor",
            "png": "src/img/vendor",
            "gif": "src/img/vendor",
            "eot":"src/fonts/vendor",
            "svg":"src/fonts/vendor",
            "ttf":"src/fonts/vendor",
            "woff":"src/fonts/vendor",
            "woff2":"src/fonts/vendor"
        },
        "sources": {
          "jquery": [
            "bower_components/jquery/dist/jquery.js"
          ],
          "bootstrap-sass":[
            "bower_components/bootstrap-sass/assets/stylesheets/bootstrap/**",
            "bower_components/bootstrap-sass/assets/javascripts/bootstrap/modal.js",
            "bower_components/bootstrap-sass/assets/javascripts/bootstrap/tooltip.js"
          ]
        }
  }
}